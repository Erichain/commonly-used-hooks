{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2019", "es2018", "es2017", "es7", "es6", "dom"],
    "declaration": true,
    "outDir": "lib",
    "strict": true,
    "jsx": "react",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": ["modules"],
  "exclude": ["node_modules", "dist", "lib"]
}
